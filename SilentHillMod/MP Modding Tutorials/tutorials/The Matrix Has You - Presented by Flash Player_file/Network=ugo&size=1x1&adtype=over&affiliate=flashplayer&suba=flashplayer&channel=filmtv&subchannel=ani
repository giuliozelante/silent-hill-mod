		popContent+='<param name=movie value="' + swfSrc +'?clickTAG=http://mediamgr.ugo.com/event.ng/Type%3dclick%26FlightID%3d30756%26AdID%3d48679%26TargetID%3d10915%26Segments%3d%26Targets%3d%26Values%3d25,31,43,51,60,72,86,101,110,150,172,202,212,547,552,557,598,619,654,730,742,743,1494,1496,1583,1763,1778,1839,1843,1862,1890,1951,1994,2077,2098,2178,2628,2637,2656,2699,2702,2703,2789,3382,3383,3384,3385,3386,3401,3402,3403,3404,3412,3413,3415,3438,3591%26RawValues%3d%26Redirect%3d' + clickThru + '"><param name=quality value=high>'
		popContent+='<embed src="' + swfSrc + '?clickTAG=http://mediamgr.ugo.com/event.ng/Type%3dclick%26FlightID%3d30756%26AdID%3d48679%26TargetID%3d10915%26Segments%3d%26Targets%3d%26Values%3d25,31,43,51,60,72,86,101,110,150,172,202,212,547,552,557,598,619,654,730,742,743,1494,1496,1583,1763,1778,1839,1843,1862,1890,1951,1994,2077,2098,2178,2628,2637,2656,2699,2702,2703,2789,3382,3383,3384,3385,3386,3401,3402,3403,3404,3412,3413,3415,3438,3591%26RawValues%3d%26Redirect%3d' + clickThru + '" quality=high swLiveConnect=false width='+ winWidth + '  height='+ winHeight + ' type="application/x-shockwave-flash" pluginspace="http://www.macromedia.com/shockwave/download/index.cgi?P1_Prod_Version=ShockwaveFlash">'
		popContent+='</embed>'
		popContent+='</object>'
		popContent+='</body></html>'
        newPopup.document.write(popContent);
        newPopup.document.close();
        newPopup.blur();
 } else if (!(navigator.appName && navigator.appName.indexOf("Netscape") >= 0 && navigator.appVersion.indexOf("2.") >= 0)) {
